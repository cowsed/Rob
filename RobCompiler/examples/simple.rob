module Main exposing Robot
import Debug

-- an example program to work on testing the compiler
program = 
	{ components = {}
	, inputs = {
		, PrintTicker: Time.every (Unit.Millisecond 100)
		}
	, outputs = [Debug.text]
	, update = update
	, init = init
}


type alias Robot 
	= Yup

init: () -> (Robot, Command)
init _ = 
	Yup, Command.None


update: Robot -> Message -> (Robot, Command) 
update robot msg = 
	case msg of 
		PrintTicker -> (robot, Debug.text "ticked")